                input(type="text" value="мал прих").form-field
              .semantist-keys-top-triple__column
                input(type="text" value="мал прих").form-field
              .semantist-keys-top-triple__column
                input(type="text" value="мал прих").form-field
              .semantist-keys-top-triple__column._button
